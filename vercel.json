{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "astro",
  "regions": ["iad1"],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PUBLIC_SITE_URL": "https://databridge.chanansystems.co.ke",
    "PUBLIC_BASE_PATH": "/"
  }
}
